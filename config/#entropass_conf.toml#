'i' = ['1', '!', '']
'j' = ['_l', '_/']
'k' = ['l<', 'l(', 'l{', 'l<=']
'l' = ['!', 'l_', '1']
'm' = ['l\\/l', '/\\/\\', 'l\\l\\', '^^']
'n' = ['l/l', '/\\/']
'o' = ['0', '()', '<>', '*', 'ø', 'Ó', '°', '']
'p' = ['l*', 'l>', '|D', 'l^', 'l+']
'q' = ['&', '(\\)', '¶']
