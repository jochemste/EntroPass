[common-replacements] # Based on 1337 dictionary
# Single backslashes are escape characters, which is why a double
# backslash is needed to represent a single backslash. e.q. '/\'=='/\\'
'a' = ['@', '4', '^', '/\\', '/-\\', ''] # = @, 4, ^, /\\, /-\\
'b' = ['l3', '8', '3', 'l', 'ß']
